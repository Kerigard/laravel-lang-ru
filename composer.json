{
    "name": "kerigard/laravel-lang-ru",
    "description": "Ru lang for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladislav Sidelnikov",
            "email": "kerihoros@gmail.com"
        }
    ],
    "homepage": "https://github.com/kerigard/laravel-lang-ru",
    "keywords": [
        "laravel"
    ],
    "require": {
        "php": "^8.2",
        "illuminate/translation": "^11.0",
        "illuminate/console": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/http": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Kerigard\\LaravelLangRu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kerigard\\LaravelLangRu\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kerigard\\LaravelLangRu\\LangRuServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
